{
  "name": "rugaard/meta-scraper",
  "type": "library",
  "description": "Scrape URL's for meta data, like Open Graph, Twitter Cards and more.",
  "keywords": [
    "rugaard",
    "scraper",
    "meta",
    "namespaces",
    "open graph",
    "facebook",
    "twitter",
    "shareable",
    "links"
  ],
  "license": "MIT",
  "homepage": "https://github.com/rugaard/meta-scraper",
  "support": {
    "issues": "https://github.com/rugaard/meta-scraper/issues",
    "source": "https://github.com/rugaard/meta-scraper"
  },
  "authors": [
    {
      "name": "Morten Rugaard",
      "email": "morten@rugaard.me",
      "homepage": "http://github.com/rugaard"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "tightenco/collect": "^5.3",
    "guzzlehttp/guzzle": "~6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0"
  },
  "autoload": {
    "psr-4": {
      "Rugaard\\MetaScraper\\": "src/"
    },
    "files": [
      "src/Support/Helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "phpunit": "./vendor/bin/phpunit"
  }
}
